{
  "compilerOptions": {
    "lib": ["deno.window", "dom"],
    "module": "esnext",
    "moduleResolution": "bundler"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  },
  "tasks": {
    "dev": "deno run --watch main.ts"
  },
  "lint": {
    "rules": {
      "tags": ["recommended"]
    }
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 2,
    "singleQuote": false
  }
}







